#!/bin/sh

MACROS_VERSION="1.0-SNAPSHOT"


DEVEL_DIR="/home/max/devel/macros"
JAR_DIR="${DEVEL_DIR}/build/libs"
JAR_FILE="macros-${MACROS_VERSION}.jar"
JAR_PATH="${JAR_DIR}/${JAR_FILE}"


unset _JAVA_OPTIONS

JAVA_ARGS="-enableassertions"

#USE_AOT_LIB=0
#if [[ $USE_AOT_LIB ]]; then
#	JAVA_ARGS="$JAVA_ARGS 
#fi


java ${JAVA_ARGS} -jar "$JAR_PATH" "$@"
